{
  "files.exclude": {
    "**/*.aux": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.gz": true,
    "**/*.log": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.dvi": true,
    "**/*.synctex*": true,
    "**/*.out*": true
  },
  "[latex]": {
    "editor.wordWrap": "on"
  },
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex"
  ]
}
